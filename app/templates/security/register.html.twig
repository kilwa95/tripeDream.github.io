{% extends 'base_security.html.twig' %}
{% block body %}

<div id="back">
    <canvas id="canvas" class="canvas-back"></canvas>
    <div class="backRight">
    </div>
    <div class="backLeft">
    </div>
</div>

<div class="right">
    <div class="content">
        <h2>Sign Up</h2>
        {{ form_start(form) }}
        <div class="form-element form-stack">
            {{ form_widget(form) }}
        </div>
        <div class="form-element form-submit">
            <button id="signUp" class="signup" type="submit"
                name="signup">{{ button_label|default('Sign Up') }}</button>
            <a href="{{path('app_login')}}" class="signup off">Log In</a>
        </div>
        {{ form_end(form) }}
    </div>
    {% endblock %}